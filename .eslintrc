{
  "extends": [
    "skyscanner",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "cypress",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true,
    "cypress/globals": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": [0]
  }
}
