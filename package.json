{
  "name": "echaloasuerte",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "start:integration": "REACT_APP_ENV=test npm run build && REACT_APP_ENV=test PORT=3005 HTTPS=false npm run start",
    "start:integration:dev": "REACT_APP_ENV=test PORT=3005 HTTPS=false npm run dev",
    "build": "next build",
    "export": "next export",
    "test": "jest --watch --testPathIgnorePatterns config/test.js",
    "test:ci": "jest --ci --testPathIgnorePatterns config/test.js",
    "test:integration:dev": "cypress open & npm run start:integration:dev",
    "test:integration": "cypress open & npm run start:integration",
    "test:integration:ci": "npm run start:integration & wait-on http://localhost:3005 && cypress run",
    "lint:scss": "stylelint 'components/**/*.module.scss' --syntax scss",
    "lint:scss:fix": "stylelint  'components/**/*.module.scss' --syntax scss --fix",
    "lint:js": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@date-io/moment": "^1.1.0",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/utils": "^4.5.2",
    "@next/bundle-analyzer": "^9.3.6",
    "@sentry/browser": "^5.10.2",
    "@sentry/node": "^5.10.2",
    "@sentry/webpack-plugin": "^1.11.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "chalk": "^4.0.0",
    "classnames": "^2.2.5",
    "echaloasuerte-js-sdk": "0.0.12",
    "express": "^4.17.1",
    "i18next": "^15.0.7",
    "js-cookie": "^2.2.0",
    "mixpanel-browser": "^2.26.0",
    "moment": "^2.24.0",
    "next": "^9.3.6",
    "next-fonts": "^1.1.0",
    "next-i18next": "^4.3.0",
    "next-images": "^1.4.0",
    "next-transpile-modules": "^3.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-countdown": "^2.2.1",
    "react-dom": "^16.8.1",
    "react-ga": "^2.5.3",
    "react-mixpanel": "^1.0.1",
    "react-redux": "^7.1.0",
    "react-share": "^2.4.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.5",
    "ua-parser-js": "^0.7.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/react": "^9.3.2",
    "cypress": "^3.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react-hooks": "^2.1.1",
    "nock": "^12.0.3",
    "prettier": "^1.17.1",
    "source-map-explorer": "^1.6.0",
    "wait-on": "^3.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
}
